{
  "name": "set-input-files",
  "description": "set HTMLInputElement files",
  "keywords": [
    "HTMLInputElement",
    "files",
    "FileList"
  ],
  "version": "1.0.1",
  "main": "lib/set-input-files.min.js",
  "module": "lib/set-input-files.min.mjs",
  "unpkg": "lib/set-input-files.min.js",
  "jsdelivr": "lib/set-input-files.min.js",
  "repository": "git@github.com:fisker/set-input-files.git",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "license": "MIT",
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "clean": "rimraf lib",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s clean lint build test format dist",
    "test": "ava",
    "test-coverage": "nyc ava"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "@commitlint/cli": "8.1.0",
    "@fisker/commitlint-config": "1.1.2",
    "@fisker/eslint-config": "1.2.18",
    "@fisker/husky-config": "1.1.2",
    "@fisker/lint-staged-config": "1.0.5",
    "@fisker/prettier-config": "1.0.20",
    "ava": "2.3.0",
    "babel-plugin-transform-async-to-promises": "0.8.14",
    "create-file-list": "1.0.0",
    "create-test-server": "3.0.1",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.3.0",
    "esm": "3.2.25",
    "husky": "3.0.5",
    "lint-staged": "9.2.5",
    "markdownlint-cli": "0.18.0",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "prettier": "1.18.2",
    "puppeteer": "1.19.0",
    "rimraf": "3.0.0",
    "rollup": "1.21.2",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "5.1.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "verbose": true,
    "require": "esm"
  },
  "nyc": {
    "all": true,
    "require": "esm",
    "reporter": [
      "lcov",
      "text"
    ],
    "include": [
      "src/**"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "sideEffects": false
}
